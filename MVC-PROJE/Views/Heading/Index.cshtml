@using EntityLayer.Concrate
@model List<Heading>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Index</h2>
<table class="table table-hover">
    <tr>
        <th>ID</th>
        <th>Başlık</th>
        <th>Kategori</th>
    </tr>
    @foreach(var item in Model )
    {
<tr>
    <th>@item.HeadingID</th>
    <td>@item.HeadingName</td>
    <td>@item.Category.CategoryName</td>

</tr>
    }
</table>